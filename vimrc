set expandtab
set tabstop=4
set shiftwidth=4
set autoindent
set smartindent
set number
set nowrap
set list
set hlsearch
set colorcolumn=80
filetype indent on
colorscheme elflord

nnoremap <F2> :set nonumber! nolist! <CR>
map <F3> :set shiftwidth=2 tabstop=2 <CR>
map <F4> :set shiftwidth=4 tabstop=4 <CR>

source /home/dmaodsigue/env/lineup.vim
